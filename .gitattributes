# Auto detect text files and perform LF normalization
* text=auto
Library/ArtifactDB filter=lfs diff=lfs merge=lfs -text
Library/PackageCache/**/*.so filter=lfs diff=lfs merge=lfs -text
Library/PackageCache/**/*.dll filter=lfs diff=lfs merge=lfs -text
Library/PackageCache/**/*.dylib filter=lfs diff=lfs merge=lfs -text
